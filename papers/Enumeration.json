 "paragraphs": 
  {
	"netbios-codes":
	{
		"type":"table",
		"canonicals": ["enumeration/netbios/codes"],
		"name"   : "codes",   
		"title"  : "Codes & Types",
		"class": "data-value",
		"columns": [{"text":"Name"},{"text":"Code"},{"text":"Type"},{"text":"Information Obtained"}],
		"rows"   : 
				  [
				   [{"text":"`<host name>`","md":true},{"text":"<00>"},{"text":"UNIQUE"},{"text":"Hostname"}],
				   [{"text":"`<domain>`","md":true},{"text":"<00>"},{"text":"GROUP"},{"text":"Domain name"}],
				   [{"text":"`<host name>`","md":true},{"text":"<03>"},{"text":"UNIQUE"},{"text":"Messenger service running for that computer"}],
				   [{"text":"`<username>`","md":true},{"text":"<03>"},{"text":"UNIQUE"},{"text":"Messenger service running for that computer"}],
				   [{"text":"`<host name>`","md":true},{"text":"<20>"},{"text":"UNIQUE"},{"text":"Server service running"}],
				   [{"text":"`<>`","md":true},{"text":"<1C>"},{"text":"UNIQUE"},{"text":"Domain controller"}],
				   [{"text":"`<domain>`","md":true},{"text":"<1D>"},{"text":"GROUP"},{"text":"Master browser name for the subnet"}],
				   [{"text":"`<hostname>`","md":true},{"text":"<1B>"},{"text":"UNIQUE"},{"text":"Domain master browser name, identifies the Primary domain controller (PDC) for that domain"}]
				  ]
    },
	"nbtstat":
	{
		"type":"table",
		"canonicals": ["enumeration/netbios/tool"],
		"name"   : "Nbtstat",   
		"title"  : "Nbtstat syntax",
		"class": "data-value",
		"columns": [{"text":"Parameter"},{"text":"Function"}],
		"rows"   : 
				  [
				   [{"text":"-a RemoteName"},{"text":"Displays the NetBIOS name table of a remote computer. RemoteName is the NetBIOS name of the remote computer."}],
				   [{"text":"-A IPAddress"},{"text":"Displays the NetBIOS name table of a remote computer, specified by the IP address."}],
				   [{"text":"-c"},{"text":"Lists the contents of the NetBIOS name cache, the table of NetBIOS names and their resolved ip addresses."}],
				   [{"text":"-n"},{"text":"Displays the names registered locally by NetBIOS applications such as the server and redirector."}],
				   [{"text":"-r"},{"text":"Displays a count of all names resolved by broadcast or WINS server."}],
				   [{"text":"-R"},{"text":"Purges the name cache and reloads all #PRE entries from LMHOSTS."}],
				   [{"text":"-RR"},{"text":"Releases and reregisters all names with the name server."}],
				   [{"text":"-s"},{"text":"Lists the NetBIOS sessions table converting destination IP addresses to computer NetBIOS names."}],
				   [{"text":"-S"},{"text":"Lists the current NetBIOS sessions and their status with the IP addresses."}],
				   [{"text":"Interval"},{"text":"Redisplays selected statistics, pausing the number of seconds specified in Interval between each display."}]
				  ]
    },
	"tools-list":
	{
			"type": "table",
